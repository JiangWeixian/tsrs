{
  "name": "tsrs-mono",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.3.0",
  "description": "",
  "keywords": [
    "typescript",
    "swc"
  ],
  "license": "MIT",
  "author": "JW",
  "napi": {
    "name": "tsrs",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "x86_64-unknown-linux-musl"
      ]
    },
    "package": {
      "name": "@tsrs/core"
    }
  },
  "scripts": {
    "test": "pnpm --filter=./crates/binding_node test:e2e",
    "artifacts": "napi artifacts --dist=crates/binding_node/npm",
    "ci:version": "pnpm changeset version",
    "ci:publish": "pnpm changeset publish",
    "ci:snapshot": "pnpm changeset version --snapshot snapshot",
    "ci:prerelease": "pnpm changeset publish --no-git-tag --snapshot",
    "lint:fix": "eslint . --fix",
    "build:all": "run-s build:*",
    "build:rust": "cargo build",
    "build:node": "pnpm --filter=./crates/binding_node run build",
    "build": "pnpm --filter=./crates/binding_node run build",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/**/*.{js,ts,tsx,json,yml,yaml,md}": ["eslint --fix"]
  },
  "devDependencies": {
    "@aiou/eslint-config": "1.5.0",
    "@changesets/cli": "^2.26.0",
    "@napi-rs/cli": "^2.18.4",
    "cz-emoji": "^1.3.1",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "npm-run-all": "4.1.5",
    "typescript": "^4.6.4"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
